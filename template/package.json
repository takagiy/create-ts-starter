{
  "name": "<%= it.projectName %>",
  "version": "1.0.0",
  "description": "",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:js",
    "build:types": "tsc --project tsconfig.build.json --declaration --emitDeclarationOnly",
    "build:js": "swc src --out-dir dist --strip-leading-paths",
    "lint": "biome check; status=$?; echo 'Use `pnpm run lint!` to apply the fixes.'; exit $status",
    "lint!": "biome check --write",
    "test": "ava",
    "version": "pnpm run lint!",
    "prepublishOnly": "pnpm run lint",
    "prepare": "pnpm run build"
  },
<% if (it.repositoryUrl) {%>
  "repository": {
    "type": "git",
    "url": "<%= it.repositoryUrl %>"
  },
<% } %>
  "keywords": [],
  "author": "<%= it.author %>",
  "license": "<%= it.license %>",
<% if (it.issueUrl) {%>
  "bugs": {
    "url": "<%= it.issueUrl %>"
  },
<% } %>
<% if (it.homepageUrl) {%>
  "homepage": "<%= it.homepageUrl %>",
<% } %>
  "packageManager": "pnpm@9.4.0+sha256.b6fd0bfda555e7e584ad7e56b30c68b01d5a04f9ee93989f4b93ca8473c49c74",
  "ava": {
    "files": [
      "tests/**/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "nodeArguments": [
      "-r",
      "@swc-node/register"
    ],
    "environmentVariables": {
      "SWCRC": "true"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@swc-node/register": "^1.10.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.6",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.14.9",
    "ava": "^6.1.3",
    "typescript": "^5.5.2"
  }
}
